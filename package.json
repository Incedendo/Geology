{
  "name": "geology_ui",
  "version": "0.1.0",
  "plugins": [
    "transform-react-pug",
    "transform-react-jsx"
  ],
  "private": true,
  "dependencies": {
    "axios": "^0.17.1",
    "bootstrap": "^3.3.7",
    "cors": "^2.8.4",
    "google-maps-react": "^1.1.2",
    "isomorphic-fetch": "^2.2.1",
    "namor": "^1.0.1",
    "node-sass-chokidar": "0.0.3",
    "npm-run-all": "^4.1.2",
    "prop-types": "^15.6.1",
    "range-slider": "^1.0.2",
    "react": "^15.6.2",
    "react-bootstrap": "^0.31.5",
    "react-bootstrap-slider": "^2.1.3",
    "react-dom": "^15.6.2",
    "react-modal": "^2.4.1",
    "react-radio-buttons": "^1.2.1",
    "react-router": "^4.2.0",
    "react-router-config": "^1.0.0-beta.4",
    "react-router-dom": "^4.2.2",
    "react-select": "^1.2.1",
    "react-slider": "^0.9.0",
    "react-static-google-map": "^0.5.1",
    "react-table": "^6.8.0",
    "reactrangeslider": "^3.0.6",
    "surge": "^0.19.0"
  },
  "devDependencies": {
    "babel-plugin-transform-react-jsx": "^6.24.1",
    "babel-plugin-transform-react-pug": "^3.2.0",
    "react-scripts": "1.0.10"
  },
  "scripts": {
    "build-css": "node-sass-chokidar src/ -o src/",
    "watch-css": "npm run build-css && node-sass-chokidar src/ -o src/ --watch --recursive",
    "start-js": "react-scripts start",
    "start": "npm-run-all -p watch-css start-js",
    "build": "npm run build-css && react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "deploy": "npm run build && surge -p build -d geology-ui.surge.sh"
  }
}
